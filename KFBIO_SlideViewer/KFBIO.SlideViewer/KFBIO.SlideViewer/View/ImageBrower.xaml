<UserControl x:Class="KFBIO.SlideViewer.ImageBrower"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:KFBIO.SlideViewer"
             xmlns:uc="http://www.quickzip.org/UserControls" 
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <uc:Explorer2 Name="explr" Style="{DynamicResource qz_Explorer_Style}" Background="{Binding Background, ElementName=mainWindow}" />
    </Grid>
</UserControl>
